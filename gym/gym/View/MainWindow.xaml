<Window x:Class="gym.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gym"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="300">
    <Grid>
        <Canvas Margin="0,10,10,10">
            <Button Style="{DynamicResource menuBtns}" Content="Judges" Canvas.Left="10" Canvas.Top="58" Click="Button_Click"></Button>
            <Button Style="{DynamicResource menuBtns}" Content="Gymnasts" Canvas.Left="65" Canvas.Top="58" RenderTransformOrigin="4.366,1"></Button>
            <Button Style="{DynamicResource menuBtns}" Content="Doctors" Canvas.Left="135" Canvas.Top="58" RenderTransformOrigin="0.4,-0.05"></Button>
            <Button Style="{DynamicResource menuBtns}" Content="Paramedics" Canvas.Left="202" Canvas.Top="58"></Button>
        </Canvas>
    </Grid>
</Window>
